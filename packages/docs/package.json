{
  "name": "@blockchain-lab-um/masca-docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prebuild": "rimraf .docusaurus build",
    "build": "docusaurus build",
    "postbuild": "pnpm compress",
    "clear": "docusaurus clear",
    "compress": "precompress build --types gz --include html,css,js,map",
    "deploy": "docusaurus deploy",
    "docker:build": "docker build . -t blockchain-lab-um/masca-docs",
    "docusaurus": "docusaurus",
    "lint": "pnpm lint:eslint && pnpm lint:prettier",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier . --check",
    "prettier": "prettier --write .",
    "serve": "docusaurus serve",
    "start": "docusaurus start",
    "swizzle": "docusaurus swizzle",
    "write-heading-ids": "docusaurus write-heading-ids",
    "write-translations": "docusaurus write-translations"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@blockchain-lab-um/masca-connector": "*",
    "@blockchain-lab-um/masca-types": "*",
    "@docusaurus/core": "2.3.1",
    "@docusaurus/preset-classic": "2.3.1",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@ethersproject/providers": "^5.7.2",
    "@mdx-js/react": "1.6.22",
    "@metamask/providers": "^10.2.1",
    "@mui/icons-material": "^5.11.9",
    "@mui/material": "^5.11.10",
    "@web3-react/core": "^6.1.9",
    "@web3-react/injected-connector": "^6.0.7",
    "clsx": "^1.2.1",
    "ethers": "^5.7.2",
    "prism-react-renderer": "^1.3.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "web3": "^1.8.2"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^2.3.1",
    "@docusaurus/plugin-content-docs": "^2.3.1",
    "@ianvs/prettier-plugin-sort-imports": "^3.7.1",
    "@tsconfig/docusaurus": "^1.0.6",
    "@types/react": "18.0.30",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "lint-staged": "^13.1.2",
    "precompress": "^10.0.2",
    "prettier": "^2.8.4",
    "prettier-plugin-packagejson": "^2.4.3",
    "rimraf": "^4.4.0",
    "stylelint": "^15.3.0",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^31.0.0",
    "typescript": "^4.9.5"
  },
  "nx": {
    "namedInputs": {
      "production": [
        "{projectRoot}/**/*",
        "!{projectRoot}/**/*.spec.ts",
        "!{projectRoot}/tsconfig.eslint.ts",
        "!{projectRoot}/.lintstagedrc.js",
        "!{projectRoot}/README.md"
      ]
    },
    "targets": {
      "build": {
        "inputs": [
          "production",
          "^production"
        ],
        "dependsOn": [
          "^build"
        ],
        "outputs": [
          "{projectRoot}/build",
          "{projectRoot}/.docusaurus"
        ]
      },
      "deploy": {
        "inputs": [
          "production",
          "^production"
        ],
        "dependsOn": [
          "^build"
        ],
        "outputs": [
          "{projectRoot}/build",
          "{projectRoot}/.docusaurus"
        ]
      },
      "start": {
        "inputs": [
          "production",
          "^production"
        ],
        "dependsOn": [
          "^build"
        ],
        "outputs": [
          "{projectRoot}/build",
          "{projectRoot}/.docusaurus"
        ]
      }
    }
  }
}
