  "nx": {
    "namedInputs": {
      "default": ["{projectRoot}/**/*"],
      "production": [
        "default",
        "!{projectRoot}/sonar-project.properties",
        "!{projectRoot}/.next/**/*",
        "!{projectRoot}/.lintstagedrc.cjs",
        "!{projectRoot}/Dockerfile",
        "!{projectRoot}/.env.*"
      ]
    },
    "targets": {
      "build": {
        "inputs": ["production", "^production"],
        "dependsOn": ["^build"],
        "outputs": ["{projectRoot}/.next"]
      },
      "start": {
        "inputs": ["production", "^production"],
        "dependsOn": ["build"],
        "outputs": []
      }
    }
